height=float(input("Enter your height in meters : "))
weight=int(input("Enter your weight in kg : "))
bmi=weight/(height*height)

if bmi<18:
    print("underweight")
elif bmi>=18 and bmi<25:
    print("normal")
elif bmi>=25 and bmi<30:
    print("overweight")
elif bmi>=30 and bmi<35:
    print("obese")

